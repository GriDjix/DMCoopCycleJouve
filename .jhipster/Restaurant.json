{
  "applications": ["coopcycle"],
  "changelogDate": "20230409213522",
  "dto": "mapstruct",
  "embedded": false,
  "entityTableName": "restaurant",
  "fields": [
    {
      "fieldName": "id",
      "fieldType": "UUID",
      "fieldValidateRules": ["unique", "required"]
    },
    {
      "fieldName": "name",
      "fieldType": "String",
      "fieldValidateRules": ["required", "minlength", "maxlength"],
      "fieldValidateRulesMaxlength": "50",
      "fieldValidateRulesMinlength": "3"
    },
    {
      "fieldName": "description",
      "fieldType": "String",
      "fieldValidateRules": ["maxlength"],
      "fieldValidateRulesMaxlength": "200"
    },
    {
      "fieldName": "address",
      "fieldType": "String",
      "fieldValidateRules": ["required", "minlength", "maxlength"],
      "fieldValidateRulesMaxlength": "100",
      "fieldValidateRulesMinlength": "5"
    },
    {
      "fieldName": "city",
      "fieldType": "String",
      "fieldValidateRules": ["required", "minlength", "maxlength"],
      "fieldValidateRulesMaxlength": "50",
      "fieldValidateRulesMinlength": "3"
    },
    {
      "fieldName": "country",
      "fieldType": "String",
      "fieldValidateRules": ["required", "minlength", "maxlength"],
      "fieldValidateRulesMaxlength": "50",
      "fieldValidateRulesMinlength": "3"
    },
    {
      "fieldName": "phone",
      "fieldType": "String",
      "fieldValidateRules": ["required", "pattern"],
      "fieldValidateRulesPattern": "^(\\+\\d{1,3}\\s)?\\(?\\d{3}\\)?[\\s.-]?\\d{3}[\\s.-]?\\d{4}$"
    },
    {
      "fieldName": "email",
      "fieldType": "String",
      "fieldValidateRules": ["required", "pattern"],
      "fieldValidateRulesPattern": "^([a-zA-Z0-9_.-]+)@([a-zA-Z0-9_-]+)\\.([a-zA-Z]{2,5})$"
    }
  ],
  "fluentMethods": true,
  "jpaMetamodelFiltering": false,
  "name": "Restaurant",
  "pagination": "no",
  "readOnly": false,
  "relationships": [
    {
      "otherEntityName": "meal",
      "otherEntityRelationshipName": "restaurant",
      "relationshipName": "meal",
      "relationshipType": "one-to-many"
    },
    {
      "otherEntityName": "order",
      "otherEntityRelationshipName": "restaurant",
      "relationshipName": "order",
      "relationshipType": "one-to-many"
    }
  ],
  "service": "serviceClass"
}
